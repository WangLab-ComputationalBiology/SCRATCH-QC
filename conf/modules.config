process {

    withName: "CELLRANGER_.*" {
        publishDir = [
            path: { "${params.outdir}/${params.project_name}" },
            mode: 'copy',
            overwrite: true
        ]
        stageInMode = "symlink"
        
    }

    withName: "QUARTO_RENDER_PAGE.*" {
        publishDir = [
            path: { "${params.outdir}/${params.project_name}" },
            pattern: '*.RDS',
            mode: 'copy'
        ]
    }

    withName: "QUARTO_RENDER_PAGEC" {
        ext.when = { params.skip_python == false }
    }

}
